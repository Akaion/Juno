<Project Sdk="Microsoft.NET.Sdk">

    <PropertyGroup>
        <TargetFramework>netstandard2.0</TargetFramework>
        <PackageVersion>1.3.1</PackageVersion>
        <Authors>Akaion</Authors>
        <Description>A Windows managed method detouring library that supports both x86 and x64 detours.</Description>
        <RepositoryUrl>https://github.com/Akaion/Juno</RepositoryUrl>
        <RepositoryType>git</RepositoryType>
    </PropertyGroup>

    <PropertyGroup Condition=" '$(Configuration)' == 'Release' ">
      <DocumentationFile>bin\Release\Juno.xml</DocumentationFile>
    </PropertyGroup>

</Project>
